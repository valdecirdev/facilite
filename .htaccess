
<IfModule mod_rewrite.c>
RewriteEngine on



RewriteRule ^$ view/index.php [L]
RewriteRule ^home?$ view/index.php [NC,L]
RewriteRule ^search?$ view/search.php [NC,L]
RewriteRule ^erro?$ view/_includes/error.php [NC,L]
RewriteRule ^identifique-se?$ view/login.php [NC,L]
RewriteRule ^cadastre-se?$ view/register.php [NC,L]
RewriteRule ^configuracoes$ view/configs.php [NC,L]
RewriteRule ^([@,a-z,0-9,A-Z,_-]+)?$ view/profile.php [NC,L]



</IfModule>